#This is test line to enable push 2
pipeline{
    agent any
    stages{
        stage("Welcome Message"){
            steps{
                echo 'Hello World. Today is $(date)'
            }
        }
        stage("Git checkout"){
            steps{
                git credentialsId: 'Git Login', url: 'https://github.com/kpunde/test'
            }
        }
    }
}
